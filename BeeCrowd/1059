            int[] numbers = new int[101];

            for (int i = 1; i < numbers.Length; i++)
            {
                numbers[i] = i;
            }

            for (int j = 1; j < numbers.Length; j++)
            {
                int x = numbers[j] % 2;

                if (x == 0)
                {
                    Console.WriteLine(numbers[j]);
                }
            }

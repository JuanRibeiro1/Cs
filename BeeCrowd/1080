            int numero;
            int numeroMaior = 0;
            int posicao = 0;

            for (int i = 0; i < 100; i++)
            {
                numero = int.Parse(Console.ReadLine());

                if (numero > numeroMaior)
                {
                    numeroMaior = numero;
                    posicao = i;
                }
            }
            Console.WriteLine(numeroMaior);
            Console.WriteLine(posicao+1);

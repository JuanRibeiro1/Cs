
            int x = 0;

            int[] number = new int[5];

            for (int i = 0; i < number.Length; i++)
            {
                number[i] = int.Parse(Console.ReadLine());
            }

            for (int i = 0; i < number.Length; i++)
            {
                if (number[i] % 2 == 0)
                {
                    x++;
                }
            }

            Console.WriteLine($"{x} valores pares");

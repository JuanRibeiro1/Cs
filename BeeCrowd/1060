            int x = 0;

            double[] number = new double[6];

            for (int i = 0; i < number.Length; i++)
            {
                number[i] = double.Parse(Console.ReadLine());
            }

            for (int i = 0; i<number.Length; i++)
            {
                if (number[i] > 0)
                {
                    x++;
                }
            }

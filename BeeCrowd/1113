            int x = 1;
            int y = 0;

            while (x != y)
            {
                string[] numeros = Console.ReadLine().Split(' ');

                x = int.Parse(numeros[0]);
                y = int.Parse(numeros[1]);

                if (x > y)
                {
                    Console.WriteLine("Decrescente");
                }
                else if (x < y)
                {
                    Console.WriteLine("Crescente");
                }

            }
